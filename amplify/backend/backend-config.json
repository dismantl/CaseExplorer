{
	"hosting": {
		"S3AndCloudFront": {
			"service": "S3AndCloudFront",
			"providerPlugin": "awscloudformation"
		}
	},
	"function": {
		"caseexplorerlambda": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true
		},
		"echofunction": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true
		}
	},
	"api": {
		"caseexplorerapi": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "caseexplorerlambda",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		},
		"caseexplorergraphql": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"securityType": "API_KEY"
			}
		}
	}
}